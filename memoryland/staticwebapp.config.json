{
  "navigationFallback": {
    "rewrite": "/",
    "exclude": ["*.{png,jpg,jpeg,gif,ico,css,scss,svg,js}"]
  },
  "routes": [
    {
      "route": "/unity/index.html",
      "redirect": "/unity/index.html"
    },
    {
      "route": "/unity/Build/*",
      "headers": {
        "Cache-Control": "public, max-age=86400, immutable"
      }
    }
  ]
}
